{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/app.js",
    "prestart:dev": "npm run clean",
    "start:dev": "tsc-watch --outDir ./build --onSuccess \"node ./build/app.js\"",
    "prestart:test": "npm run clean",
    "start:test": "tsc-watch --outDir ./build --onSuccess \"npm run test\"",
    "pretest": "npm run db:test up",
    "test": "cross-env NODE_ENV=test jasmine",
    "posttest": "npm run db:test reset",
    "init": "node ./build/init.js",
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf ./build",
    "prettier": "prettier ./src/**/*.ts --write",
    "lint": "eslint ./src/**/*.ts --ext .ts",
    "lint:fix": "eslint ./src/**/*.ts --ext .ts --fix",
    "db": "db-migrate -e production",
    "db:dev": "db-migrate -e development",
    "db:test": "db-migrate -e test"
  },
  "author": "Eslam Gaber",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jasmine": "^3.6.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.25",
    "@types/pg": "^7.14.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
